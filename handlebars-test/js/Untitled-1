var template = $('#handlebars-demo').html();
    var context = {
        "name": {
            firstName: "Ben",
            lastName: "Harker"}
        , "occupation": "Front-End Web Developer"
        , "hobbies": [
            {activity: "Photography", location: 'Oregon'}, 
            {activity: 'Hiking', location: 'The Tetons'}, 
            {activity: 'Biking', location: 'Kansas'}
        ]
    };
    var out = [];
    Handlebars.registerHelper('list', function (items, options) {
        for (team in items) {
            out += options.fn(items[team]);
        }
        return out;
    });
    Handlebars.registerHelper('fullname', function (name){
    return name.firstName + ' ' + name.lastName});
    
    var templateScript = Handlebars.compile(template);
    var html = templateScript(context);
    
    {{#each teams}}
        {{name}}//"Team 1"
        {{team_lead}}//"Zach Williams"
        {{#each team_members}}
            {{member_name}}//"Mathew Jones"
            {{full_time_weight}}
            {{ticket_count}}
            {{load}}
            {{#each courses}}
                {{course_name}}
                {{course_lead}}
                {{section}}
                {{ticket}}
                {{department}}
                {{score}}
                {{group_name}}
            {{/courses}}
        {{/team_members}}
    {{/teams}}